{
    "name": "csrdelft/csrdelft.nl",
    "description": "C.S.R. Delft webstek",
    "authors": [
        {
            "name": "PubCie",
            "email": "pubcie@csrdelft.nl"
        }
    ],
    "keywords": [
        "stek"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "csrdelft/orm": "^1.7",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^4.0",
        "globalcitizen/php-iban": "^2.6",
        "google/apiclient": "^2.0",
        "jacwright/restserver": "dev-master",
        "jakeasmith/http_build_url": "^1",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "maknz/slack": "^1.7",
        "oyejorge/less.php": "v1.7.0.14",
        "robmorgan/phinx": "^0.9",
        "smarty/smarty": "^3.1",
        "tedivm/jshrink": "^1.1",
        "tedivm/stash": "^0.14.1",
        "tubalmartin/cssmin": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "CsrDelft\\": "lib/"
        },
        "files": [
            "lib/defines.include.php",
            "lib/common.functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
           "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
           "@php artisan key:generate"
        ],
        "post-autoload-dump": [
           "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
           "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
