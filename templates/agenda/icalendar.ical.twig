BEGIN:VCALENDAR
PRODID:-//C.S.R. Delft//C.S.R. agenda//NL
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-ORIGINAL-URL:{{ url('csrdelft_agenda_maand') }}
X-WR-CALNAME:C.S.R. agenda
X-WR-TIMEZONE:Europe/Amsterdam
X-WR-CALDESC:
BEGIN:VTIMEZONE
TZID:Europe/Amsterdam
X-LIC-LOCATION:Europe/Amsterdam
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
END:STANDARD
END:VTIMEZONE
{# IcalResponse trimt witruimte #}
{% for item in items %}
    BEGIN:VEVENT
    {% if item.heledag %}
        DTSTART;VALUE=DATE:{{ date(item.beginMoment) | date('Ymd') }}
    {% else %}
        DTSTART;TZID=Europe/Amsterdam:{{ date(item.beginMoment) | date('Ymd\\THis') }}
    {% endif %}
    {% if item.heledag %}
        DTEND;VALUE=DATE:{{ date(item.eindMoment) | date('Ymd') }}
    {% else %}
        DTEND;TZID=Europe/Amsterdam:{{ date(item.eindMoment) | date('Ymd\\THis') }}
    {% endif %}
    DTSTAMP:{{ published }}
    UID:{{ item.uUID }}
    ORGANIZER:C.S.R. Delft
    CREATED:{{ published }}
    DESCRIPTION:{{ item.beschrijving }}
    LAST-MODIFIED:{{ published }}
    LOCATION:{{ item.locatie }}
    SEQUENCE:0
    STATUS:CONFIRMED
    SUMMARY:{{ item.titel }}
    {% if item.transparant %}
        TRANSP:TRANSPARENT
    {% else %}
        TRANS:OPAQUE
    {% endif %}
    END:VEVENT
{% endfor %}
END:VCALENDAR
{{- "" -}}
