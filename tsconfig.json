{
	"compilerOptions": {
		"target": "es2017",
		"module": "esnext",
		"lib": ["DOM", "ES2019"],
		"moduleResolution": "node",
		"removeComments": false,
		"types": ["@types/jqueryui"],
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true
	},
	"include": ["assets/js/**/*.ts", "assets/js/**/*.vue"],
	"compileOnSave": false
}
