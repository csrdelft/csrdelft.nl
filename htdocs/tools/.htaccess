RewriteEngine On
RewriteRule ^leden.csv$ leden-csv.php

RewriteRule ^pasfoto/([0-9a-z]+).png$ pasfotos.php?uid=$1&image
RewriteRule ^naamsuggesties/([a-z]+)/?$ naamsuggesties.php?zoekin=$1 [QSA]
RewriteRule ^suggesties/([a-z]+)/?$ suggesties.php?datatype=$1 [QSA]
RewriteRule ^suggesties/([a-z]+)/([0-9]+)/?$ suggesties.php?datatype=$1&categorie=$2 [QSA]